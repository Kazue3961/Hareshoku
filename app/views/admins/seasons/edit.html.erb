<div class="container-fluid">
  <div class="row">
    <div class="col-xs-12">
      <h1>行事季節編集</h1>
        <%= form_for(@season, url:admins_season_path(@season.id)) do |f| %>
          <%= f.label "行事季節" %>
          <%= f.text_field :name %>
          <%= f.submit '変更を保存', class: "btn btn-primary btn-md" %>
        <% end %>

    </div>
  </div>
</div>